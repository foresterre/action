{
  "name": "publish-crate",
  "version": "0.0.0",
  "private": true,
  "description": "publish new versions of crates to crates.io",
  "main": "lib/main.js",
  "scripts": {
    "build": "ncc build src/main.ts --minify",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license LICENSE",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "watch": "ncc build src/main.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foresterre/action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Martijn Gribnau",
  "license": "MIT",
  "dependencies": {
    "@actions-rs/core": "^0.1.5",
    "@actions/core": "^1.2.5",
    "@iarna/toml": "^2.2.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.9.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.7"
  }
}
